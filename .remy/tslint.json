{
  "extends": "tslint-config-airbnb",
  "rules": {
    "align": [false, "parameters"],
    "variable-name": [true, "allow-pascal-case"],
    "import-name": [false],
    "object-shorthand-properties-first": [false],
    "max-line-length": [true, 200],
    "no-trailing-whitespace": false
  },
  "linterOptions": {
    "exclude": [
      "**/*.js",
      "**/*.json"
    ]
  }
}
